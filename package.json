{
  "name": "auxw-tool",
  "version": "0.0.1-alpha-2025-10-25",
  "description": "AuxW_Tool",
  "main": "./out/main/index.js",
  "author": "Boom W. W.",
  "scripts": {
    "postinstall": "electron-builder install-app-deps && node scripts/dev_scripts/post_install.mjs",
    "prepare": "husky",
    "precommit": "lint-staged && node scripts/dev_scripts/pre_commit.mjs",
    "commit": "git-cz && node scripts/dev_scripts/post_commit.mjs",
    "lint": "npm run lint:prettier && npm run lint:eslint && npm run lint:spell",
    "lint:prettier": "prettier --write .",
    "lint:eslint": "eslint --cache .",
    "lint:spell": "cspell lint .",
    "clear": "node scripts/dev_scripts/clear.mjs",
    "pre:dev": "node scripts/dev_scripts/pre_dev.mjs",
    "dev": "npm run pre:dev && concurrently -k \"npm run dev:docs\" \"npm run dev:electron\"",
    "dev:docs": "vitepress dev src/docs",
    "dev:electron": "electron-vite dev",
    "start": "electron-vite preview",
    "pre:build": "node scripts/build_scripts/pre_build.mjs",
    "build": "npm run pre:build && npm run build:vite && electron-builder --win && npm run post:build",
    "build:unpack": "npm run pre:build && npm run build:vite && electron-builder --dir",
    "build:docs": "vitepress build src/docs",
    "build:vite": "npm run build:docs && npm run typecheck && electron-vite build",
    "post:build": "node scripts/build_scripts/post_build.mjs",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false"
  },
  "dependencies": {
    "adm-zip": "^0.5.16",
    "express": "^5.1.0",
    "fs-extra": "11.3.2",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@cspell/dict-lorem-ipsum": "^4.0.5",
    "@electron-toolkit/eslint-config-prettier": "3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.1.0",
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@electron-toolkit/utils": "^4.0.0",
    "@types/adm-zip": "^0.5.7",
    "@types/express": "^5.0.3",
    "@types/node": "^22.18.6",
    "@vitejs/plugin-vue": "^6.0.1",
    "commitizen": "^4.3.1",
    "concurrently": "^9.2.1",
    "cspell": "^9.2.1",
    "cz-git": "^1.12.0",
    "electron": "^38.1.2",
    "electron-builder": "^25.1.8",
    "electron-vite": "^4.0.1",
    "eslint": "^9.36.0",
    "eslint-plugin-vue": "^10.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "pinia": "^3.0.3",
    "pixi-live2d-display": "0.4.0",
    "pixi.js": "6.5.10",
    "prettier": "^3.6.2",
    "sass-embedded": "^1.93.2",
    "typescript": "^5.9.2",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-vue-components": "^29.1.0",
    "vite": "^7.1.6",
    "vitepress": "2.0.0-alpha.12",
    "vue": "^3.5.21",
    "vue-eslint-parser": "^10.2.0",
    "vue-router": "^4.5.1",
    "vue-tsc": "^3.0.7"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0",
    "pnpm": ">=10.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "esbuild",
      "@parcel/watcher"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
